{
  "name": "terminal_chatapp",
  "version": "1.0.0",
  "description": "A command line chat app",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node dist/client.js",
    "client": "babel-node -- client.js",
    "build": "npm run clean && babel client.js --out-dir ./dist",
    "clean": "rm -rf dist && mkdir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kleva-j/Terminal_ChatApp.git"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "engines": {
    "node": "10.x"
  },
  "keywords": [
    "command-line"
  ],
  "author": "Michael Obasi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kleva-j/Terminal_ChatApp/issues"
  },
  "homepage": "https://github.com/kleva-j/Terminal_ChatApp#readme",
  "dependencies": {
    "@babel/polyfill": "^7.2.3",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "prompt": "^1.0.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.11.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  }
}
